@charset "utf-8";
/* CSS Document */

.view{
	position:relative;
	width:950px;
	height:350px;
	margin: 25px auto;
	text-align:center;
}
/*.hideSel{
	position:relative;
	display:none;
}*/
#status th{ 
font: bold 11px "Trebuchet MS", Verdana, Arial, Helvetica, sans-serif; 
color: #4f6b72; 
border: 1px solid #C1DAD7; 
letter-spacing: 2px; 
text-transform: uppercase; 
text-align: center; 
padding: 4px 2px; 
background: #CAE8EA  no-repeat; 
}
#ctrlPanel{
	padding-bottom:5px;
}
#status .link:hover {
	cursor:pointer;
	color:#00F;
}
#ss_container {
	position:relative;
	width:100%;
	margin:0px auto;
}
#ss_container #page{
	position:relative;
    text-align:center;
    clear: both;
	min-width:960px;
    width:auto !important;
	margin:10px 5px 15px 15px;
    
    min-height:350px;
    height:auto !important; 
	background: #F7F7F7;
 padding: 4px 10px 5px 10px;
 border: 1px solid #fff;
 border-bottom: 1px solid #363d50;
 -moz-border-radius: 10px;
 -webkit-border-radius: 10px;
 border-radius: 10px;
 -moz-box-shadow:
  inset 0 4px 7px 0 #ffffff,
  inset 0 -2px 4px 0 rgba(194, 194, 194, 0.4),
  inset 0 -4px 8px 0 rgba(194, 194, 194, 0.9),
  0 2px 4px 0 rgba(0, 0, 0, 0.4);
 -webkit-box-shadow:
  inset 0 4px 7px 0 #ffffff,
  inset 0 -2px 4px 0 rgba(194, 194, 194, 0.4),
  inset 0 -4px 8px 0 rgba(194, 194, 194, 0.9),
  0 2px 4px 0 rgba(0, 0, 0, 0.4);
 box-shadow:
  inset 0 4px 7px 0 #ffffff,
  inset 0 -2px 4px 0 rgba(194, 194, 194, 0.4),
  inset 0 -4px 8px 0 rgba(194, 194, 194, 0.9),
  0 2px 4px 0 rgba(0, 0, 0, 0.4);
}
#service_status{
	position:relative;
	padding-bottom:10px;
}
#service_show{
	position:relative;
	text-align:center;
	display:none;
}
#quickMenu{
	position:relative;
	float:right;
	margin-top:6px;
	width:360px;
}
#quickMenu span{
	position:relative;
	display:block;
	float:left;
	margin:2px 5px;
}
#quickMenu .qBtn:hover{
	cursor:pointer;
	color:#00F;	
}
#quickMenu .qBtn.on{
	color:#00F;
}
.nosee{
	display: none;
}
.compare_1{
	background:#F8E9DA;
}
.compare_2{
	background:#E4F8DA;
}
.upd{
	color:#C33;
}
.equ{
	color:#66F;
}
.dwd{
	color:#0C6;
}
